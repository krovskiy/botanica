{
  "manifest_version": 3,
  "name": "botanica",
  "version": "0.1",
  "description": "Take high-quality QHD screenshots with Chrome DevTools for printing, PDFs, or presentations!",
  "permissions": [
    "activeTab",
    "debugger",
    "scripting",
    "downloads",
    "tabs"
  ],
  "action": {
    "default_popup": "botanica/botanica.html",
    "default_title": "botanica"
  },
  "host_permissions": [
  "<all_urls>"
],
"commands": {
    "open-extension": {
      "suggested_key": {
        "default": "Alt+B",
        "mac": "Alt+B"
      },
      "description": "Open the extension screenshot"
    }
  }
}